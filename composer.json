{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "advoor/nova-editor-js": "dev-temporaryurl",
        "aws/aws-sdk-php": "^3.261",
        "ebess/advanced-nova-media-library": "^4.1",
        "fakerphp/faker": "^1.21",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.15",
        "laravel/nova": "~4.0",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^2.1",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.9",
        "spatie/laravel-tags": "^4.3",
        "spatie/nova-tags-field": "^4.0",
        "stancl/tenancy": "^3.7",
        "staudenmeir/belongs-to-through": "^2.5",
        "vyuldashev/nova-permission": "^3.2",
        "whitecube/nova-flexible-content": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.4",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.2",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "nova-editor-js": {
            "type": "vcs",
            "url": "https://github.com/hansenhalim/nova-editor-js"
        },
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
